<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mapper.IProductDao">
    <resultMap id="productResultMap" type="com.Pojo.ProductTypePojo">
        <id property="typeId" column="type_id"/>
        <result property="typeName" column="type_name"/>
        <result property="typeState" column="type_state"/>
    </resultMap>
    <select id="findAllProduct" resultMap="productResultMap">
        select * from product_type
    </select>
    <insert id="addProduct">
        <selectKey keyProperty="typeId" keyColumn="type_id" resultType="java.lang.String" order="BEFORE">
            select replace(uuid(),'-','')
        </selectKey>
        insert into product_type values (#{typeId},#{typeName},#{typeState})
    </insert>
    <delete id="">
    </delete>

</mapper>